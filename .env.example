# Pinata Configuration
# Get your JWT token from: https://app.pinata.cloud/developers/api-keys
PINATA_JWT_TOKEN=your_pinata_jwt_token_here

# Optional: Custom IPFS gateway (defaults to https://gateway.pinata.cloud)
PINATA_GATEWAY=https://gateway.pinata.cloud

# Stellar and NFT contract vars
STELLAR_NETWORK=testnet
STELLAR_SOURCE_ACCOUNT=admin

STELLAR_NFT_CONTRACT_ID=
STELLAR_NFT_CONTRACT_NAME=nft
STELLAR_NFT_CONTRACT_SYMBOL=TEST
STELLAR_NFT_CONTRACT_MAX_SUPPLY=1
STELLAR_NFT_CONTRACT_BASE_URI=https://ipfs.io/ipfs/to-be-replaced

# For IPFS images upload, first add the images to be uploaded in the scripts/assets/images folder
