  "scripts": {
    "setup-dev-env-vars": "sh -c '[ -f src/config/.env.local ] || cp src/config/.env.example src/config/.env.local'",
    "setup-test-env-vars": "sh -c '[ -f src/config/.env.test ] || cp src/config/.env.example src/config/.env.test'",
    "setup-dev": "npm install && npm run setup-dev-env-vars && npm run setup-test-env-vars",
    "clean-setup-dev": "npm ci && npm run setup-dev-env-vars && npm run setup-test-env-vars",
    "clean-setup": "npm ci --production",
    "build": "tsc && vite build",
    "start": "vite --port ${PORT:-3000} --host;",
    "start-dev": "npm start",
    "start-staging": "npm start -- --mode staging",
    "serve": "npx serve -l ${PORT:-3000} -s dist",
    "test": "vitest --config vitest.config.ts",
    "test-coverage": "vitest run --config vitest.config.ts --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
